{
	"breadcrumbs.enabled": false,
	"color-highlight.markRuler": false,
	"editor.colorDecorators": false,
	"editor.tabSize": 4,
	"editor.fontSize": 14,
	"editor.wordWrap": "on",
	"editor.formatOnSave": true,
	"editor.insertSpaces": false,
	"editor.tabCompletion": "on",
	"editor.fontLigatures": true,
	"editor.smoothScrolling": true,
	"editor.minimap.enabled": false,
	"editor.cursorBlinking": "solid",
	"editor.renderWhitespace": "none",
	"editor.detectIndentation": false,
	"editor.suggestSelection": "first",
	"editor.renderLineHighlight": "all",
	"editor.fontFamily": "FiraCode-Retina",
	"editor.acceptSuggestionOnEnter": "on",
	"editor.scrollbar.verticalScrollbarSize": 0,
	"editor.quickSuggestions": {
		"other": true,
		"strings": true,
		"comments": true
	},
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"emmet.includeLanguages": {
		"php": "html",
		"vue-html": "html",
		"javascript": "javascriptreact",
		"typescript": "typescriptreact"
	},
	"eslint.probe": [
		"vue",
		"html",
		"markdown",
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact"
	],
	"explorer.confirmDelete": false,
	"explorer.openEditors.visible": 0,
	"explorer.confirmDragAndDrop": false,
	"files.autoSave": "afterDelay",
	"files.trimFinalNewlines": true,
	"files.autoGuessEncoding": true,
	"files.insertFinalNewline": false,
	"files.trimTrailingWhitespace": true,
	"files.exclude": {
		"**/node_modules": true
	},
	"javascript.format.semicolons": "insert",
	"launch": {
		"version": "0.2.0",
		"configurations": [
			{
				"type": "node",
				"request": "launch",
				"name": "Launch Program",
				"program": "${file}"
			}
		]
	},
	"terminal.integrated.fontSize": 12,
	"terminal.external.osxExec": "iTerm.app",
	"terminal.integrated.cursorStyle": "line",
	"terminal.integrated.fontFamily": "FiraCode-Retina",
	"terminal.integrated.shell.osx": "/usr/local/bin/zsh",
	"git.enabled": false,
	"[go]": {
		"editor.codeActionsOnSave": {
			"source.organizeImports": true
		}
	},
	"go.alternateTools": {
		"go-langserver": "gopls"
	},
	"go.buildOnSave": "package",
	"go.formatTool": "goimports",
	"go.formatFlags": ["-w"],
	"go.gocodeAutoBuild": true,
	"go.installDependenciesWhenBuilding": true,
	"go.languageServerExperimentalFeatures": {
		"hover": true,
		"format": true,
		"rename": true,
		"diagnostics": true,
		"autoComplete": true,
		"signatureHelp": true,
		"goToDefinition": true,
		"findReferences": true,
		"documentSymbols": true,
		"workspaceSymbols": true,
		"goToTypeDefinition": true,
		"goToImplementation": true
	},
	"go.lintTool": "golint",
	"go.lintOnSave": "package",
	"gopls": {
		"deepCompletion": true,
		"enhancedHover": false,
		"usePlaceholders": false,
		"watchFileChanges": true,
		"completeUnimported": true
	},
	"go.testOnSave": false,
	"go.useLanguageServer": true,
	"go.vetOnSave": "package",
	"rust-client.channel": "stable",
	"vetur.format.options.tabSize": 8,
	"vetur.format.options.useTabs": true,
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"workbench.tips.enabled": false,
	"workbench.colorTheme": "Ayu Dark",
	"workbench.settings.editor": "json",
	"workbench.statusBar.visible": false,
	"workbench.fontAliasing": "antialiased",
	"workbench.editor.closeOnFileDelete": true,
	"workbench.iconTheme": "material-icon-theme",
	"workbench.colorCustomizations": {
		"activityBar.border": "#0A0E14",
		"activityBar.foreground": "#626A73",
		"activityBar.background": "#0A0E14",
		"activityBarBadge.foreground": "#0A0E14",
		"badge.foreground": "#0A0E14",
		"button.foreground": "#0A0E14",
		"dropdown.foreground": "#626A73",
		"editor.background": "#0A0E14",
		"editorLineNumber.foreground": "#626A73",
		"editorGroupHeader.tabsBorder": "#0A0E14",
		"editor.findRangeHighlightBorder": "#0A0E14",
		"editorGroupHeader.tabsBackground": "#0A0E14",
		"editorLineNumber.activeForeground": "#F0F0F0",
		"editorGroupHeader.noTabsBackground": "#0A0E14",
		"extensionButton.prominentForeground": "#0A0E14",
		"foreground": "#626A73",
		"list.activeSelectionForeground": "#F0F0F0",
		"list.inactiveSelectionForeground": "#F0F0F0",
		"tab.border": "#0A0E14",
		"tab.focusForeground": "#F0F0F0",
		"tab.activeBackground": "#0A0E14",
		"tab.activeForeground": "#F0F0F0",
		"tab.inactiveBackground": "#0A0E14",
		"tab.inactiveForeground": "#626A73",
		"terminal.background": "#0A0E14",
		"titleBar.border": "#0A0E14",
		"titleBar.activeBackground": "#0A0E14",
		"titleBar.inactiveBackground": "#0A0E14",
		"input.foreground": "#626A73",
		"input.placeholderForeground": "#626A73",
		"inputValidation.errorBackground": "#0A0E14",
		"inputValidation.infoBackground": "#0A0E14",
		"inputValidation.warningBackground": "#0A0E14",
		"statusBar.border": "#0A0E14",
		"statusBar.foreground": "#626A73",
		"statusBar.background": "#0A0E14",
		"statusBar.debuggingForeground": "#0A0E14",
		"sideBar.border": "#0A0E14",
		"sideBar.background": "#0A0E14",
		"sideBar.foreground": "#626A73",
		"sideBarTitle.foreground": "#626A73",
		"sideBarSectionHeader.foreground": "#626A73",
		"sideBarSectionHeader.background": "#0A0E14",
		"panel.background": "#0A0E14",
		"panelTitle.activeForeground": "#F0F0F0",
		"panelTitle.inactiveForeground": "#626A73"
	},
	"window.zoomLevel": -1,
	"workbench.activityBar.visible": false
}
